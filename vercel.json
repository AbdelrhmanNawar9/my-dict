{
  "version": 2,
  "builds": [
    {
      "src": "./app.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": "templates/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "app.py": {
      "includeFiles": "templates/**"
    }
  }
}
